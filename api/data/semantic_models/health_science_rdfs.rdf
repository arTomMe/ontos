<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <rdfs:Class rdf:about="http://example.org/health/Disease">
    <rdfs:label>Disease</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://example.org/health/InfectiousDisease">
    <rdfs:subClassOf rdf:resource="http://example.org/health/Disease"/>
    <rdfs:label>Infectious Disease</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://example.org/health/NonInfectiousDisease">
    <rdfs:subClassOf rdf:resource="http://example.org/health/Disease"/>
    <rdfs:label>Non-Infectious Disease</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://example.org/health/Treatment">
    <rdfs:label>Treatment</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://example.org/health/Medication">
    <rdfs:subClassOf rdf:resource="http://example.org/health/Treatment"/>
    <rdfs:label>Medication</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:about="http://example.org/health/Therapy">
    <rdfs:subClassOf rdf:resource="http://example.org/health/Treatment"/>
    <rdfs:label>Therapy</rdfs:label>
  </rdfs:Class>

</rdf:RDF>


