@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Ontos Application Vocabulary
@prefix ontos: <http://ontos.app/vocabulary#> .
@prefix odcs: <http://odcs.io/vocabulary#> .
@prefix corp: <http://example.com/corporate/taxonomy#> .
@prefix concept: <http://example.com/business/concepts#> .
@prefix prop: <http://example.com/business/properties#> .

# ============================================================================
# ONTOS CORE VOCABULARY
# Comprehensive RDFS vocabulary for mapping Ontos application objects
# into a unified knowledge graph supporting semantic search and governance
# ============================================================================

# Ontology Header
ontos: a owl:Ontology ;
    rdfs:label "Ontos Application Vocabulary" ;
    rdfs:comment "RDFS vocabulary for mapping Ontos data governance application objects into a knowledge graph" ;
    owl:versionInfo "1.0.0" ;
    dc:created "2024-09-22" ;
    dc:creator "Ontos Data Governance Platform" ;
    dc:conformsTo "https://github.com/bitol-io/open-data-contract-standard/blob/main/schema/odcs-json-schema-v3.0.2.json" .

# ============================================================================
# ROOT CLASSES
# ============================================================================

ontos:Entity a rdfs:Class ;
    rdfs:label "Entity" ;
    rdfs:comment "Root class for all Ontos application entities" .

# ============================================================================
# HIERARCHICAL DATA STRUCTURE: Domain > Product > Contract > Schema > Property
# This models the logical containment hierarchy using rdfs:subClassOf
# ============================================================================

ontos:OrganizationalUnit a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Organizational Unit" ;
    rdfs:comment "Base class for organizational structures that contain and organize data assets" .

ontos:DataDomain a rdfs:Class ;
    rdfs:subClassOf ontos:OrganizationalUnit ;
    rdfs:label "Data Domain" ;
    rdfs:comment "Logical organizational unit for grouping related data products and assets" ;
    skos:example "Customer Domain, Financial Domain, Product Domain" .

ontos:DataProduct a rdfs:Class ;
    rdfs:subClassOf ontos:DataDomain ;
    rdfs:label "Data Product" ;
    rdfs:comment "Curated collection of Databricks assets belonging to a domain, grouped by tags" ;
    skos:example "Customer 360 View, Real-time Fraud Detection" ;
    skos:definition "Products are contained within domains and contain contracts" .

ontos:DataContract a rdfs:Class ;
    rdfs:subClassOf ontos:DataProduct ;
    rdfs:label "Data Contract" ;
    rdfs:comment "ODCS-compliant specification belonging to a product, defining technical metadata and governance" ;
    skos:example "Customer Profile Contract, Transaction Data Contract" ;
    skos:definition "Contracts are contained within products and contain schema objects" .

ontos:SchemaObject a rdfs:Class ;
    rdfs:subClassOf ontos:DataContract ;
    rdfs:label "Schema Object" ;
    rdfs:comment "ODCS schema object belonging to a contract, representing tables, views, or other structured data containers" ;
    skos:definition "Schema objects are contained within contracts and contain schema properties" .

ontos:SchemaProperty a rdfs:Class ;
    rdfs:subClassOf ontos:SchemaObject ;
    rdfs:label "Schema Property" ;
    rdfs:comment "ODCS schema property belonging to a schema object, with logical/physical types and constraints" ;
    skos:definition "Properties are contained within schema objects and represent individual columns or fields" .

# ============================================================================
# ORGANIZATIONAL UNITS
# ============================================================================

ontos:Team a rdfs:Class ;
    rdfs:subClassOf ontos:OrganizationalUnit ;
    rdfs:label "Team" ;
    rdfs:comment "Collection of users and groups with specific roles and domain assignments" ;
    skos:example "Data Engineering Team, Analytics Team" .

ontos:Project a rdfs:Class ;
    rdfs:subClassOf ontos:OrganizationalUnit ;
    rdfs:label "Project" ;
    rdfs:comment "Workspace container that assigns teams to specific initiatives with isolation boundaries" ;
    skos:example "Customer Analytics Platform, Risk Reporting System" .

ontos:TeamMember a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Team Member" ;
    rdfs:comment "Individual user or group membership within a team with optional role overrides" .

# ============================================================================
# ODCS ROOT CLASS
# ============================================================================

ontos:ODCSEntity a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "ODCS Entity" ;
    rdfs:comment "Root class for all Open Data Contract Standard entities" ;
    skos:definition "Base class for ODCS-compliant objects including servers, teams, roles, and SLA properties" .

# ============================================================================
# ODCS SERVER CLASSES
# ============================================================================

ontos:Server a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "Server" ;
    rdfs:comment "ODCS server configuration where datasets physically reside" ;
    skos:definition "Base class for all ODCS server types with connection details" .

# ODCS Server Types
ontos:ApiServer a rdfs:Class ;
