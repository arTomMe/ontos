    rdfs:label "Azure Storage Server" ;
    rdfs:comment "Azure Blob Storage or ADLS server configuration" .

ontos:BigQueryServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "BigQuery Server" ;
    rdfs:comment "Google BigQuery server configuration" .

ontos:ClickHouseServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "ClickHouse Server" ;
    rdfs:comment "ClickHouse database server configuration" .

ontos:DatabricksServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Databricks Server" ;
    rdfs:comment "Databricks Unity Catalog server configuration" .

ontos:DenodoServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Denodo Server" ;
    rdfs:comment "Denodo data virtualization server configuration" .

ontos:DremioServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Dremio Server" ;
    rdfs:comment "Dremio data lake engine server configuration" .

ontos:DuckDBServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "DuckDB Server" ;
    rdfs:comment "DuckDB analytical database server configuration" .

ontos:GlueServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "AWS Glue Server" ;
    rdfs:comment "AWS Glue Data Catalog server configuration" .

ontos:KafkaServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Kafka Server" ;
    rdfs:comment "Apache Kafka streaming platform server configuration" .

ontos:KinesisServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Kinesis Server" ;
    rdfs:comment "AWS Kinesis Data Streams server configuration" .

ontos:LocalServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Local Server" ;
    rdfs:comment "Local file system server configuration" .

ontos:MySQLServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "MySQL Server" ;
    rdfs:comment "MySQL database server configuration" .

ontos:OracleServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Oracle Server" ;
    rdfs:comment "Oracle database server configuration" .

ontos:PostgreSQLServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "PostgreSQL Server" ;
    rdfs:comment "PostgreSQL database server configuration" .

ontos:PrestoServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Presto Server" ;
    rdfs:comment "Presto distributed SQL query engine server configuration" .

ontos:RedshiftServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Redshift Server" ;
    rdfs:comment "Amazon Redshift data warehouse server configuration" .

ontos:S3Server a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "S3 Server" ;
    rdfs:comment "Amazon S3 object storage server configuration" .

ontos:SftpServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "SFTP Server" ;
    rdfs:comment "SFTP file transfer server configuration" .

ontos:SnowflakeServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Snowflake Server" ;
    rdfs:comment "Snowflake cloud data platform server configuration" .

ontos:SqlServerServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "SQL Server" ;
    rdfs:comment "Microsoft SQL Server database server configuration" .

ontos:SynapseServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Synapse Server" ;
    rdfs:comment "Azure Synapse Analytics server configuration" .

ontos:TrinoServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Trino Server" ;
    rdfs:comment "Trino distributed SQL query engine server configuration" .

ontos:VerticaServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Vertica Server" ;
    rdfs:comment "Vertica analytical database server configuration" .

ontos:CustomServer a rdfs:Class ;
    rdfs:subClassOf ontos:Server ;
    rdfs:label "Custom Server" ;
    rdfs:comment "Custom or vendor-specific server configuration" .

# ============================================================================
# ODCS TEAM AND ROLE STRUCTURES
# ============================================================================

ontos:TeamMemberDefinition a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "Team Member Definition" ;
    rdfs:comment "ODCS team member with username, role, and lifecycle information" ;
    skos:definition "Detailed team member as defined in ODCS specification" .

ontos:RoleDefinition a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "Role Definition" ;
    rdfs:comment "ODCS IAM role definition with access rights and approvers" ;
    skos:definition "Role-based access control as defined in ODCS specification" .

ontos:SLAProperty a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "SLA Property" ;
    rdfs:comment "ODCS Service Level Agreement property with value and constraints" ;
    skos:definition "Service level agreement property as defined in ODCS specification" .

ontos:SupportChannel a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "Support Channel" ;
    rdfs:comment "ODCS support channel definition with contact information" ;
    skos:definition "Support channel as defined in ODCS specification" .

ontos:PricingInfo a rdfs:Class ;
    rdfs:subClassOf ontos:ODCSEntity ;
    rdfs:label "Pricing Information" ;
    rdfs:comment "ODCS pricing information with amount, currency, and unit" ;
    skos:definition "Pricing information as defined in ODCS specification" .

# ============================================================================
# GOVERNANCE ARTIFACTS
# ============================================================================

ontos:GovernanceArtifact a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Governance Artifact" ;
    rdfs:comment "Base class for governance, compliance, and documentation artifacts" .

ontos:BusinessGlossary a rdfs:Class ;
    rdfs:subClassOf ontos:GovernanceArtifact ;
    rdfs:label "Business Glossary" ;
    rdfs:comment "Hierarchical collection of business terms with organizational scope" ;
    skos:definition "Supports inheritance from corporate → LOB → department → team levels" .

ontos:GlossaryTerm a rdfs:Class ;
    rdfs:subClassOf ontos:GovernanceArtifact ;
    rdfs:label "Glossary Term" ;
    rdfs:comment "Business term definition with synonyms, examples, and tagged assets" ;
    skos:example "Customer, Revenue, Product Category" .

ontos:AssetReview a rdfs:Class ;
    rdfs:subClassOf ontos:GovernanceArtifact ;
    rdfs:label "Asset Review" ;
    rdfs:comment "Approval workflow for Databricks assets including review status and comments" .

ontos:QualityRule a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Quality Rule" ;
    rdfs:comment "ODCS quality rule specifying validation logic and monitoring requirements" ;
    skos:example "Email format validation, Customer ID uniqueness check" .

# ============================================================================
# SEMANTIC LINKING
# ============================================================================

ontos:SemanticLink a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Semantic Link" ;
    rdfs:comment "Association between application entities and ontology concepts via IRIs" ;
    skos:definition "Enables semantic enrichment and knowledge graph navigation" .

ontos:OntologyConcept a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Ontology Concept" ;
    rdfs:comment "RDF/RDFS concept from loaded taxonomies with hierarchical relationships" .

ontos:SemanticModel a rdfs:Class ;
    rdfs:subClassOf ontos:Entity ;
    rdfs:label "Semantic Model" ;
    rdfs:comment "Conceptual model linking data structures to business semantics and ontology concepts" .

# ============================================================================
# OBJECT PROPERTIES - ORGANIZATIONAL RELATIONSHIPS
# ============================================================================

ontos:belongsToDomain a rdf:Property ;
    rdfs:label "belongs to domain" ;
    rdfs:comment "Associates an entity with its parent data domain" ;
    rdfs:domain ontos:Entity ;
    rdfs:range ontos:DataDomain .

ontos:assignedToTeam a rdf:Property ;
    rdfs:label "assigned to team" ;
    rdfs:comment "Associates a project with one or more assigned teams" ;
    rdfs:domain ontos:Project ;
    rdfs:range ontos:Team .

ontos:hasParentDomain a rdf:Property ;
    rdfs:label "has parent domain" ;
    rdfs:comment "Defines hierarchical relationship between data domains" ;
    rdfs:domain ontos:DataDomain ;
    rdfs:range ontos:DataDomain .

ontos:memberOfTeam a rdf:Property ;
    rdfs:label "member of team" ;
    rdfs:comment "Associates a team member with their team" ;
    rdfs:domain ontos:TeamMember ;
    rdfs:range ontos:Team .

# ============================================================================
# OBJECT PROPERTIES - HIERARCHICAL DATA RELATIONSHIPS
# ============================================================================

ontos:containsProduct a rdf:Property ;
    rdfs:label "contains product" ;
    rdfs:comment "Associates a data domain with its contained data products" ;
    rdfs:domain ontos:DataDomain ;
    rdfs:range ontos:DataProduct .

ontos:containsContract a rdf:Property ;
    rdfs:label "contains contract" ;
    rdfs:comment "Associates a data product with its contained data contracts" ;
    rdfs:domain ontos:DataProduct ;
    rdfs:range ontos:DataContract .

ontos:containsSchemaObject a rdf:Property ;
    rdfs:label "contains schema object" ;
    rdfs:comment "Associates a data contract with its contained schema objects" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:SchemaObject .

ontos:containsSchemaProperty a rdf:Property ;
    rdfs:label "contains schema property" ;
    rdfs:comment "Associates a schema object with its contained schema properties" ;
    rdfs:domain ontos:SchemaObject ;
    rdfs:range ontos:SchemaProperty .

# ============================================================================
# OBJECT PROPERTIES - ODCS RELATIONSHIPS
# ============================================================================

# ODCS Server Relationships
ontos:residesOnServer a rdf:Property ;
    rdfs:label "resides on server" ;
    rdfs:comment "Associates a data contract with the servers where datasets reside" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:Server .

# ODCS Team and Role Relationships
ontos:hasTeamMember a rdf:Property ;
    rdfs:label "has team member" ;
    rdfs:comment "Associates a data contract with its team member definitions" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:TeamMemberDefinition .

ontos:hasRole a rdf:Property ;
    rdfs:label "has role" ;
    rdfs:comment "Associates a data contract with its role definitions" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:RoleDefinition .

# ODCS SLA and Support Relationships
ontos:hasSLAProperty a rdf:Property ;
    rdfs:label "has SLA property" ;
    rdfs:comment "Associates a data contract with its SLA properties" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:SLAProperty .

ontos:hasSupportChannel a rdf:Property ;
    rdfs:label "has support channel" ;
    rdfs:comment "Associates a data contract with its support channels" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:SupportChannel .

# ODCS Pricing Relationship
ontos:hasPricing a rdf:Property ;
    rdfs:label "has pricing" ;
    rdfs:comment "Associates a data contract with its pricing information" ;
    rdfs:domain ontos:DataContract ;
    rdfs:range ontos:PricingInfo .

# Nested Property Structures for ODCS Compliance
ontos:hasNestedProperty a rdf:Property ;
    rdfs:label "has nested property" ;
    rdfs:comment "Associates a schema property with its nested properties (for object types)" ;
    rdfs:domain ontos:SchemaProperty ;
    rdfs:range ontos:SchemaProperty .

ontos:hasArrayItem a rdf:Property ;
    rdfs:label "has array item" ;
    rdfs:comment "Associates a schema property with its array item schema (for array types)" ;
    rdfs:domain ontos:SchemaProperty ;
    rdfs:range ontos:SchemaProperty .

ontos:hasQualityRule a rdf:Property ;
    rdfs:label "has quality rule" ;
    rdfs:comment "Associates an entity with its quality validation rules" ;
    rdfs:domain ontos:Entity ;
    rdfs:range ontos:QualityRule .

# ============================================================================
# OBJECT PROPERTIES - SEMANTIC RELATIONSHIPS
# ============================================================================

ontos:hasSemanticConcept a rdf:Property ;
    rdfs:label "has semantic concept" ;
    rdfs:comment "Links an entity to ontology concepts for semantic enrichment" ;
    rdfs:domain ontos:Entity ;
    rdfs:range ontos:OntologyConcept .

ontos:linkedToIRI a rdf:Property ;
    rdfs:label "linked to IRI" ;
    rdfs:comment "Direct reference to external ontology concept IRI" ;
    rdfs:domain ontos:SemanticLink .

ontos:derivedFromGlossary a rdf:Property ;
    rdfs:label "derived from glossary" ;
    rdfs:comment "Links an ontology concept to its source glossary term" ;
    rdfs:domain ontos:OntologyConcept ;
    rdfs:range ontos:GlossaryTerm .

ontos:parentGlossary a rdf:Property ;
    rdfs:label "parent glossary" ;
    rdfs:comment "Defines hierarchical inheritance between business glossaries" ;
    rdfs:domain ontos:BusinessGlossary ;
    rdfs:range ontos:BusinessGlossary .

# ============================================================================
# OBJECT PROPERTIES - GOVERNANCE RELATIONSHIPS
# ============================================================================

ontos:reviewedBy a rdf:Property ;
    rdfs:label "reviewed by" ;
    rdfs:comment "Associates an asset with its review process" ;
    rdfs:domain ontos:Entity ;
    rdfs:range ontos:AssetReview .

ontos:taggedWithConcept a rdf:Property ;
    rdfs:label "tagged with concept" ;
    rdfs:comment "Associates a glossary term with tagged Databricks assets" ;
    rdfs:domain ontos:GlossaryTerm .

# ============================================================================
# DATA PROPERTIES - IDENTIFIERS AND NAMES
# ============================================================================

ontos:entityId a rdf:Property ;
    rdfs:label "entity ID" ;
    rdfs:comment "Unique identifier for application entities" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:name a rdf:Property ;
    rdfs:label "name" ;
    rdfs:comment "Human-readable name of the entity" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:displayTitle a rdf:Property ;
    rdfs:label "display title" ;
    rdfs:comment "Optional display title for presentation purposes" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:physicalName a rdf:Property ;
    rdfs:label "physical name" ;
    rdfs:comment "Physical implementation name (e.g., table name, column name)" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - DESCRIPTIONS AND METADATA
# ============================================================================

ontos:description a rdf:Property ;
    rdfs:label "description" ;
    rdfs:comment "Human-readable description of the entity" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:purpose a rdf:Property ;
    rdfs:label "purpose" ;
    rdfs:comment "Intended purpose or business objective" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:usage a rdf:Property ;
    rdfs:label "usage" ;
    rdfs:comment "Intended usage patterns and guidelines" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:limitations a rdf:Property ;
    rdfs:label "limitations" ;
    rdfs:comment "Known limitations or constraints" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - LIFECYCLE AND STATUS
# ============================================================================

ontos:lifecycle a rdf:Property ;
    rdfs:label "lifecycle" ;
    rdfs:comment "Current lifecycle stage of the entity" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string ;
    skos:example "draft, active, deprecated, retired" .

ontos:status a rdf:Property ;
    rdfs:label "status" ;
    rdfs:comment "Current operational status" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:version a rdf:Property ;
    rdfs:label "version" ;
    rdfs:comment "Version identifier for versioned entities" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - ORGANIZATIONAL SCOPE
# ============================================================================

ontos:organizationalScope a rdf:Property ;
    rdfs:label "organizational scope" ;
    rdfs:comment "Organizational level of applicability" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string ;
    skos:example "company, lob, department, team" .

ontos:organizationalUnit a rdf:Property ;
    rdfs:label "organizational unit" ;
    rdfs:comment "Specific organizational unit identifier" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - OWNERSHIP AND RESPONSIBILITY
# ============================================================================

ontos:owner a rdf:Property ;
    rdfs:label "owner" ;
    rdfs:comment "Primary owner or responsible party" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:createdBy a rdf:Property ;
    rdfs:label "created by" ;
    rdfs:comment "User who created the entity" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

ontos:updatedBy a rdf:Property ;
    rdfs:label "updated by" ;
    rdfs:comment "User who last updated the entity" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - TEMPORAL
# ============================================================================

ontos:createdAt a rdf:Property ;
    rdfs:label "created at" ;
    rdfs:comment "Timestamp when entity was created" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:dateTime .

ontos:updatedAt a rdf:Property ;
    rdfs:label "updated at" ;
    rdfs:comment "Timestamp when entity was last updated" ;
    rdfs:domain ontos:Entity ;
    rdfs:range xsd:dateTime .

# ============================================================================
# DATA PROPERTIES - TEAM AND MEMBERSHIP
# ============================================================================

ontos:memberType a rdf:Property ;
    rdfs:label "member type" ;
    rdfs:comment "Type of team member (user or group)" ;
    rdfs:domain ontos:TeamMember ;
    rdfs:range xsd:string ;
    skos:example "user, group" .

ontos:memberIdentifier a rdf:Property ;
    rdfs:label "member identifier" ;
    rdfs:comment "Email for users, name for groups" ;
    rdfs:domain ontos:TeamMember ;
    rdfs:range xsd:string .

ontos:roleOverride a rdf:Property ;
    rdfs:label "role override" ;
    rdfs:comment "Optional application role override for team member" ;
    rdfs:domain ontos:TeamMember ;
    rdfs:range xsd:string .

# ============================================================================
# DATA PROPERTIES - DATA QUALITY
# ============================================================================

ontos:qualityDimension a rdf:Property ;
    rdfs:label "quality dimension" ;
    rdfs:comment "ODCS quality dimension being measured" ;
    rdfs:domain ontos:QualityRule ;
    rdfs:range xsd:string ;
    skos:example "accuracy, completeness, conformity, consistency, coverage, timeliness, uniqueness" .

ontos:qualitySeverity a rdf:Property ;
    rdfs:label "quality severity" ;
    rdfs:comment "Severity level of quality rule violations" ;
    rdfs:domain ontos:QualityRule ;
    rdfs:range xsd:string ;
    skos:example "info, warning, error" .

ontos:businessImpact a rdf:Property ;
    rdfs:label "business impact" ;
    rdfs:comment "Business impact category of quality issues" ;
    rdfs:domain ontos:QualityRule ;
    rdfs:range xsd:string ;
    skos:example "operational, regulatory" .

# ============================================================================
# DATA PROPERTIES - SCHEMA STRUCTURE
# ============================================================================

ontos:logicalType a rdf:Property ;
    rdfs:label "logical type" ;
    rdfs:comment "ODCS logical data type" ;
    rdfs:domain ontos:SchemaProperty ;
    rdfs:range xsd:string .

ontos:physicalType a rdf:Property ;
    rdfs:label "physical type" ;
    rdfs:comment "Physical implementation data type" ;
    rdfs:domain ontos:SchemaProperty ;
    rdfs:range xsd:string .

ontos:isRequired a rdf:Property ;
    rdfs:label "is required" ;
    rdfs:comment "Whether the schema property is required (non-null)" ;
    rdfs:domain ontos:SchemaProperty ;
    rdfs:range xsd:boolean .

ontos:isUnique a rdf:Property ;
    rdfs:label "is unique" ;
    rdfs:comment "Whether the schema property must contain unique values" ;
    rdfs:domain ontos:SchemaProperty ;
