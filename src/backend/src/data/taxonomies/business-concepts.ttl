@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix concept: <http://example.com/business/concepts#> .
@prefix prop: <http://example.com/business/properties#> .
@prefix corp: <http://example.com/corporate/taxonomy#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Business Concepts Taxonomy
# High-level business entities and domain concepts

# Root concept class
concept:BusinessConcept a rdfs:Class ;
    rdfs:label "Business Concept" ;
    rdfs:comment "Base class for all business domain concepts and entities" .

# === CUSTOMER DOMAIN ===

concept:CustomerDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Customer Domain" ;
    rdfs:comment "All concepts related to customers and customer management" .

concept:Customer a rdfs:Class ;
    rdfs:subClassOf concept:CustomerDomain ;
    rdfs:label "Customer" ;
    rdfs:comment "Individual or organization that purchases products or services" ;
    concept:hasProperty prop:email ;
    concept:hasProperty prop:firstName ;
    concept:hasProperty prop:lastName ;
    concept:hasProperty prop:phoneNumber ;
    concept:hasProperty prop:dateOfBirth ;
    concept:hasProperty prop:streetAddress ;
    concept:hasProperty prop:city ;
    concept:hasProperty prop:state ;
    concept:hasProperty prop:zipCode ;
    concept:hasProperty prop:country ;
    concept:hasProperty prop:createdDate ;
    concept:hasProperty prop:status .

concept:CustomerProfile a rdfs:Class ;
    rdfs:subClassOf concept:CustomerDomain ;
    rdfs:label "Customer Profile" ;
    rdfs:comment "Detailed profile information about a customer including preferences and behavior" ;
    concept:hasProperty prop:creditScore ;
    concept:hasProperty prop:preferredChannel ;
    concept:hasProperty prop:loyaltyTier ;
    concept:hasProperty prop:riskLevel .

concept:CustomerSegment a rdfs:Class ;
    rdfs:subClassOf concept:CustomerDomain ;
    rdfs:label "Customer Segment" ;
    rdfs:comment "Classification of customers into distinct groups" ;
    concept:hasProperty prop:segmentName ;
    concept:hasProperty prop:segmentCriteria ;
    concept:hasProperty prop:targetValue .

# === FINANCIAL DOMAIN ===

concept:FinancialDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Financial Domain" ;
    rdfs:comment "All concepts related to financial operations and data" .

concept:Account a rdfs:Class ;
    rdfs:subClassOf concept:FinancialDomain ;
    rdfs:label "Account" ;
    rdfs:comment "Financial account for holding funds or tracking financial activity" ;
    concept:hasProperty prop:accountNumber ;
    concept:hasProperty prop:accountType ;
    concept:hasProperty prop:balance ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:openDate ;
    concept:hasProperty prop:status ;
    concept:hasProperty prop:isActive .

concept:Transaction a rdfs:Class ;
    rdfs:subClassOf concept:FinancialDomain ;
    rdfs:label "Transaction" ;
    rdfs:comment "Financial transaction involving transfer of funds" ;
    concept:hasProperty prop:transactionId ;
    concept:hasProperty prop:amount ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:transactionDate ;
    concept:hasProperty prop:transactionType ;
    concept:hasProperty prop:description ;
    concept:hasProperty prop:status .

concept:Payment a rdfs:Class ;
    rdfs:subClassOf concept:FinancialDomain ;
    rdfs:label "Payment" ;
    rdfs:comment "Payment made for goods or services" ;
    concept:hasProperty prop:paymentMethod ;
    concept:hasProperty prop:amount ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:paymentDate ;
    concept:hasProperty prop:status .

concept:Invoice a rdfs:Class ;
    rdfs:subClassOf concept:FinancialDomain ;
    rdfs:label "Invoice" ;
    rdfs:comment "Request for payment for goods or services provided" ;
    concept:hasProperty prop:invoiceNumber ;
    concept:hasProperty prop:amount ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:invoiceDate ;
    concept:hasProperty prop:dueDate ;
    concept:hasProperty prop:status .

# === PRODUCT DOMAIN ===

concept:ProductDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Product Domain" ;
    rdfs:comment "All concepts related to products and services" .

concept:Product a rdfs:Class ;
    rdfs:subClassOf concept:ProductDomain ;
    rdfs:label "Product" ;
    rdfs:comment "Good or service offered by the organization" ;
    concept:hasProperty prop:productId ;
    concept:hasProperty prop:productName ;
    concept:hasProperty prop:sku ;
    concept:hasProperty prop:price ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:category ;
    concept:hasProperty prop:description ;
    concept:hasProperty prop:status ;
    concept:hasProperty prop:isActive .

concept:ProductCatalog a rdfs:Class ;
    rdfs:subClassOf concept:ProductDomain ;
    rdfs:label "Product Catalog" ;
    rdfs:comment "Collection of products organized for sales and marketing" ;
    concept:hasProperty prop:catalogName ;
    concept:hasProperty prop:catalogType ;
    concept:hasProperty prop:effectiveDate ;
    concept:hasProperty prop:expirationDate .

concept:Order a rdfs:Class ;
    rdfs:subClassOf concept:ProductDomain ;
    rdfs:label "Order" ;
    rdfs:comment "Customer request to purchase products or services" ;
    concept:hasProperty prop:orderId ;
    concept:hasProperty prop:orderDate ;
    concept:hasProperty prop:totalAmount ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:status ;
    concept:hasProperty prop:shippingAddress .

# === SALES DOMAIN ===

concept:SalesDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Sales Domain" ;
    rdfs:comment "All concepts related to sales processes and activities" .

concept:Lead a rdfs:Class ;
    rdfs:subClassOf concept:SalesDomain ;
    rdfs:label "Lead" ;
    rdfs:comment "Potential customer who has shown interest in products or services" ;
    concept:hasProperty prop:leadId ;
    concept:hasProperty prop:leadSource ;
    concept:hasProperty prop:leadScore ;
    concept:hasProperty prop:status ;
    concept:hasProperty prop:createdDate ;
    concept:hasProperty prop:assignedTo .

concept:Opportunity a rdfs:Class ;
    rdfs:subClassOf concept:SalesDomain ;
    rdfs:label "Opportunity" ;
    rdfs:comment "Qualified sales prospect with potential for revenue" ;
    concept:hasProperty prop:opportunityId ;
    concept:hasProperty prop:value ;
    concept:hasProperty prop:probability ;
    concept:hasProperty prop:stage ;
    concept:hasProperty prop:closeDate ;
    concept:hasProperty prop:assignedTo .

concept:Campaign a rdfs:Class ;
    rdfs:subClassOf concept:SalesDomain ;
    rdfs:label "Campaign" ;
    rdfs:comment "Marketing or sales campaign to promote products or services" ;
    concept:hasProperty prop:campaignId ;
    concept:hasProperty prop:campaignName ;
    concept:hasProperty prop:startDate ;
    concept:hasProperty prop:endDate ;
    concept:hasProperty prop:budget ;
    concept:hasProperty prop:status .

# === EMPLOYEE DOMAIN ===

concept:EmployeeDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Employee Domain" ;
    rdfs:comment "All concepts related to employees and human resources" .

concept:Employee a rdfs:Class ;
    rdfs:subClassOf concept:EmployeeDomain ;
    rdfs:label "Employee" ;
    rdfs:comment "Person employed by the organization" ;
    concept:hasProperty prop:employeeId ;
    concept:hasProperty prop:firstName ;
    concept:hasProperty prop:lastName ;
    concept:hasProperty prop:email ;
    concept:hasProperty prop:phoneNumber ;
    concept:hasProperty prop:jobTitle ;
    concept:hasProperty prop:department ;
    concept:hasProperty prop:manager ;
    concept:hasProperty prop:hireDate ;
    concept:hasProperty prop:status ;
    concept:hasProperty prop:isActive .

concept:Department a rdfs:Class ;
    rdfs:subClassOf concept:EmployeeDomain ;
    rdfs:label "Department" ;
    rdfs:comment "Organizational unit within the company" ;
    concept:hasProperty prop:departmentId ;
    concept:hasProperty prop:departmentName ;
    concept:hasProperty prop:manager ;
    concept:hasProperty prop:budget ;
    concept:hasProperty prop:location .

# === RETAIL DOMAIN ===

concept:RetailDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Retail Domain" ;
    rdfs:comment "All concepts related to retail operations" .

concept:Store a rdfs:Class ;
    rdfs:subClassOf concept:RetailDomain ;
    rdfs:label "Store" ;
    rdfs:comment "Physical retail location" ;
    concept:hasProperty prop:storeId ;
    concept:hasProperty prop:storeName ;
    concept:hasProperty prop:streetAddress ;
    concept:hasProperty prop:city ;
    concept:hasProperty prop:state ;
    concept:hasProperty prop:zipCode ;
    concept:hasProperty prop:country ;
    concept:hasProperty prop:phoneNumber ;
    concept:hasProperty prop:manager ;
    concept:hasProperty prop:openDate ;
    concept:hasProperty prop:status .

concept:Sale a rdfs:Class ;
    rdfs:subClassOf concept:RetailDomain ;
    rdfs:label "Sale" ;
    rdfs:comment "Completed transaction at point of sale" ;
    concept:hasProperty prop:saleId ;
    concept:hasProperty prop:saleDate ;
    concept:hasProperty prop:totalAmount ;
    concept:hasProperty prop:currency ;
    concept:hasProperty prop:paymentMethod ;
    concept:hasProperty prop:cashier ;
    concept:hasProperty prop:storeId .

concept:Inventory a rdfs:Class ;
    rdfs:subClassOf concept:RetailDomain ;
    rdfs:label "Inventory" ;
    rdfs:comment "Stock of products available for sale" ;
    concept:hasProperty prop:productId ;
    concept:hasProperty prop:storeId ;
    concept:hasProperty prop:quantity ;
    concept:hasProperty prop:reorderLevel ;
    concept:hasProperty prop:lastUpdated .

# === MARKETING DOMAIN ===

concept:MarketingDomain a rdfs:Class ;
    rdfs:subClassOf concept:BusinessConcept ;
    rdfs:label "Marketing Domain" ;
    rdfs:comment "All concepts related to marketing and customer engagement" .

concept:MarketingCampaign a rdfs:Class ;
    rdfs:subClassOf concept:MarketingDomain ;
    rdfs:label "Marketing Campaign" ;
    rdfs:comment "Coordinated marketing activities to promote products or services" ;
    concept:hasProperty prop:campaignId ;
    concept:hasProperty prop:campaignName ;
    concept:hasProperty prop:channel ;
    concept:hasProperty prop:budget ;
    concept:hasProperty prop:startDate ;
    concept:hasProperty prop:endDate ;
    concept:hasProperty prop:targetAudience .

concept:CustomerInteraction a rdfs:Class ;
    rdfs:subClassOf concept:MarketingDomain ;
    rdfs:label "Customer Interaction" ;
    rdfs:comment "Any touchpoint or interaction with a customer" ;
    concept:hasProperty prop:interactionId ;
    concept:hasProperty prop:interactionType ;
    concept:hasProperty prop:channel ;
    concept:hasProperty prop:interactionDate ;
    concept:hasProperty prop:outcome ;
    concept:hasProperty prop:notes .

# Custom property to link concepts to properties
concept:hasProperty a rdf:Property ;
    rdfs:label "Has Property" ;
    rdfs:comment "Links a business concept to a business property" ;
    rdfs:domain concept:BusinessConcept ;
    rdfs:range prop:BusinessProperty .

# Additional properties for business concepts
concept:domainArea a rdf:Property ;
    rdfs:label "Domain Area" ;
    rdfs:comment "The business domain this concept belongs to" ;
    rdfs:domain concept:BusinessConcept ;
    rdfs:range xsd:string .

concept:businessOwner a rdf:Property ;
    rdfs:label "Business Owner" ;
    rdfs:comment "The business unit or person responsible for this concept" ;
    rdfs:domain concept:BusinessConcept ;
    rdfs:range xsd:string .

concept:lifecycleStage a rdf:Property ;
    rdfs:label "Lifecycle Stage" ;
    rdfs:comment "Current stage in the concept lifecycle" ;
    rdfs:domain concept:BusinessConcept ;
    rdfs:range xsd:string .

concept:criticality a rdf:Property ;
    rdfs:label "Criticality" ;
    rdfs:comment "Business criticality level of this concept" ;
    rdfs:domain concept:BusinessConcept ;
    rdfs:range xsd:string .